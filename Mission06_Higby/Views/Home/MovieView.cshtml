
@{
    ViewData["Title"] = "View Joel's Collection";
}

@using Microsoft.EntityFrameworkCore
@model List<Movie>

<table class="table table-bordered">
    <thead class="fw-bold">
        <tr>
            <td>MovieID</td>
            <td>Category</td>
            <td>Title</td>
            <td>Year</td>
            <td>Director</td>
            <td>Rating</td>
            <td>Edited</td>
            <td>LentTo</td>
            <td>CopiedToPlex</td>
            <td>Notes</td>
        </tr>
    </thead>
    
    @foreach (var x in Model)
    {
        <tbody>
            <tr>
                <td>@x.MovieID</td>
                <td>@x.CategoryID</td>
                <td>@x.Title</td>
                <td>@x.Year</td>
                <td>@x.Director</td>
                <td>@x.Rating</td>
                <td>@x.Edited</td>
                <td>@x.LentTo</td>
                <td>@x.CopiedToPlex</td>
                <td>@x.Notes</td>
            </tr>
        </tbody>
    }
</table>
